{
  "name": "spa",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "webpack --mode production --progress",
    "start": "yarn start:lintCache",
    "start:lintCache": "webpack serve --progress --mode development --env ESLINT_CACHE",
    "build:analyze": "npm run build -- --env MEASURE_SPEED --json ./.internal/bundleStats.json && webpack-bundle-analyzer ./.internal/bundleStats.json",
    "dev:analyze": "webpack  --env MEASURE_SPEED --json ./dist/bundleStats.json && webpack-bundle-analyzer ./dist/bundleStats.json",
    "test": "jest --config=./jest.config.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "lint:staged": "lint-staged",
    "pre-commit": "npm run typecheck && npm run lint:staged"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint . --fix"
    ]
  },
