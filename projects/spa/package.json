    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-formatjs": "^10.2.12",
    "babel-plugin-styled-components": "^1.12.0",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "colors": "^1.4.0",
