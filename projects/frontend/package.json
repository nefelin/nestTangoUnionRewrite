  "scripts": {
    "cy": "./node_modules/.bin/cypress run",
    "build": "REACT_APP_ENV=prod webpack --mode production --progress",
    "start:dev": "webpack serve --mode development --env ESLINT_CACHE",
    "start:prod": "REACT_APP_ENV=prod webpack serve --mode development --env ESLINT_CACHE",
    "build:analyze": "npm run build -- --env MEASURE_SPEED --json ./.internal/bundleStats.json && webpack-bundle-analyzer ./.internal/bundleStats.json",
    "dev:analyze": "webpack  --env MEASURE_SPEED --json ./dist/bundleStats.json && webpack-bundle-analyzer ./dist/bundleStats.json",
    "test": "jest --config=./jest.config.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "lint:staged": "lint-staged",
    "generate": "graphql-codegen --config codegen.yml",
    "pre-commit": "npm run typecheck && npm run lint:staged",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint . --fix"
    ]
  },
